{
    "manifest_version": 3,
    "name": "HyperVerge - Home",
    "version": "1.0",
    "description": "A digital notice board for improved team collaboration.",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": [
      "storage",
      "activeTab"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  